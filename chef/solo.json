{
    "rvm": {
        "default_ruby": "ruby-2.0.0-p353@global",
        "global_gems": [
            {
                "name": "bundler"
            },
            {
                "name": "rake"
            }
        ],
        "rvm_gem_options": "--no-ri --no-rdoc"
    },
    "nginx": {
	"init_style": "init",
	"source": {
	    "prefix": "/etc/nginx",
	    "conf_path": "/etc/nginx/nginx.conf",
	    "modules": [
		"nginx::headers_more_module",
		"nginx::passenger",
		"nginx::http_ssl_module"
	    ]
	},
	"passenger": {
	    "version": "4.0.41",
	    "gem_binary": "/usr/local/rvm/wrappers/default/gem"
	}
    },
    "mysql": {
	"server_debian_password": "root",
	"server_repl_password": "root",
	"server_root_password": "root"
    },
    "redis": {
	"server": {
	    "run_state": "start"
	}
    },
    "run_list": [
        "recipe[openssh::default]",
        "recipe[rvm::system]",
        "recipe[mysql::client]",
	"recipe[redisio::install]",
	"recipe[redisio::enable]",
	"recipe[monkey::default]",
	"recipe[nginx::source]"
    ]
}
